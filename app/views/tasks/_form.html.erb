<%= form_for @task do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :complete_by %>
  <%= f.datetime_local_field :complete_by %>

  <%= f.label :completed_at %>
  <%= f.datetime_local_field :completed_at %>

  <%= f.submit %>
<% end %>
