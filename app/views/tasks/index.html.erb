
<header id = "index_header">
  <h1>Tasks list</h1>
</header>




<main id = "index_main">
  <nav>
    <ul>
      <% @tasks.each_with_index do |task| %>
        <li>
          <%= link_to task[:name], task_path(task.id) %>
        </li>
      <% end %>
    </ul>
  </nav>

</main>





