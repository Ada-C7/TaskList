<h1>Tasks#index</h1>
<p>Find me in app/views/tasks/index.html.erb</p>

<section>
  <ul>
    <li>
    <% @tasks.each do |task| %>
      <% task %>
      </li>
      <%task.each do |k, v| %>
        <%= k %>
        <% v.each do |goals| %>
          <%= goals %>
        <% end %>
      <% end %>
    <% end %>
  </ul>
</section>
