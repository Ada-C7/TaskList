<h1>Emma's Tasks</h1>
<p>
  Enter a new task below
</p>

<%= form_for @task do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.submit %>

<% end %>

<%= link_to "go back to the homepage", welcome_path %> <br />
<%= link_to "See tasks", tasks_path %>
