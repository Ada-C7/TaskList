<h1>Emma's Tasks</h1>
<p>
  Look up a task below
</p>
<%= form_for @task do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name%>
  </p>

  <p>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>

<% link_to "Back to task list", tasks_path %>
<%= link_to "Back to the homepage", welcome_path %> <br />
